{
  "name": "Tournament",
  "description": "Tournament web app",
  "main": "src/js/app.js",
  "private": true,
  "author": "Carl Eriksson",
  "dependencies": {
    "acorn": "^6.0.0",
    "flux": "3.1.3",
    "keymirror": "0.1.1",
    "object-assign": "4.1.1",
    "react": "16.6.0",
    "roundrobin": "1.0.x"
  },
  "devDependencies": {
    "browserify": "16.2.3",
    "envify": "4.1.0",
    "reactify": "1.1.1",
    "uglify-js": "3.4.9",
    "watchify": "3.11.0"
  },
  "scripts": {
    "start": "STATIC_ROOT=./static watchify -o src/js/bundle.js -v -d .",
    "build": "STATIC_ROOT=./static NODE_ENV=production browserify . | uglifyjs -cm -o src/js/bundle.min.js"
  },
  "browserify": {
    "transform": [
      "reactify",
      "envify"
    ]
  }
}
